{
	"cSpell.enableFiletypes": [
		"mdx"
	],
	"cSpell.words": [
		"antongolub",
		"apos",
		"astro",
		"astrojs",
		"clsx",
		"codeql",
		"Deno",
		"dzharii",
		"horiz",
		"stroustrup",
		"svgs"
	],
	"emojisense.languages": {
		"git-commit": true,
		"markdown": true,
		"mdx": true,
		"plaintext": {
			"emojiDecoratorsEnabled": false,
			"markupCompletionsEnabled": false
		},
		"scminput": true
	},
	"files.associations": {
		"*.excalidraw": "json",
		"*.excalidrawlib": "json",
		"*.mdx": "markdown"
	},
	"ltex.enabled": [
		"bibtex",
		"context",
		"context.tex",
		"html",
		"latex",
		"markdown",
		"mdx",
		"org",
		"restructuredtext",
		"rsweave"
	],
	"markdownlint.ignore": ".markdownlintignore",
	"search.exclude": {
		"**/.yarn": true
	},
	"typescript.tsdk": "node_modules\\typescript\\lib"
}
